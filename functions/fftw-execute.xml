<refentry xml:id="function.fftw-execute" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>fftw_execute</refname>
  <refpurpose>Execute plan</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>fftw_execute</methodname>
   <methodparam><type>fftw_plan</type><parameter>plan</parameter></methodparam>
  </methodsynopsis>
  <para>
   Execute plan.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>plan</parameter></term>
    <listitem>
     &fftw.plan.description;
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Return TRUE when executed.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
