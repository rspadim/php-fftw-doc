<refentry xml:id="function.fftw-output" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>fftw_output</refname>
  <refpurpose>Return plan output.</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>mixed</type><methodname>fftw_output</methodname>
   <methodparam><type>fftw_plan</type><parameter>plan</parameter></methodparam>
  </methodsynopsis>
  <para>
   Return the plan (must check what C fftw function do this, maybe wisdom export?)
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>plan</parameter></term>
    <listitem>
     &fftw.plan.description;
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Must check what we return, maybe string?
  </para>
 </refsect1>

</refentry>

